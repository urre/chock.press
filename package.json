{
	"name": "chockpress",
	"version": "1.0.0",
	"description": "Chocking Swedish News",
	"main": "./src/index.js",
	"author": "Urban Sanden",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git://github.com/urre/chockpress.git"
	},
	"scripts": {
		"develop": "webpack-dev-server --config ./webpack.config.dev.js --mode development",
		"build": "NODE_ENV=production webpack --config ./webpack.config.prod.js --mode production",
		"start": "serve ./build",
		"collect": "cd src/scraper && npx babel-node scrape.js",
		"match": "cd src/scraper && npx babel-node match.js",
		"copydata": "cp -R ./src/data/ build/data",
		"scrape": "npm run collect && npm run match && npm run copydata",
		"save": "cd src/scraper/helpers/ && npx babel-node savetodb.js",
		"clean": "cd ./build && rm -rf *.js && rm -rf index.html",
		"release": "clear && npm run clean && npm run build && npm run scrape && npm run copydata && npm run now && npm run save",
		"now": "now ./build -A ../now.json && now alias && now ls --all chockpress",
		"now:remove": "now rm chockpress --safe --yes"
	},
	"devDependencies": {
		"@babel/cli": "^7.2.3",
		"@babel/core": "^7.2.2",
		"@babel/node": "^7.2.2",
		"@babel/preset-env": "^7.2.3",
		"@babel/preset-react": "^7.0.0",
		"app-reset": "^1.0.2",
		"async-series": "0.0.1",
		"axios": "^0.18.0",
		"babel-loader": "^8.0.0-beta.6",
		"chalk": "^2.4.1",
		"concat-files": "^0.1.0",
		"css-loader": "^1.0.0",
		"dotenv": "^6.2.0",
		"google-spreadsheet": "^2.0.6",
		"html-loader": "^0.5.5",
		"html-webpack-plugin": "^3.2.0",
		"http-server": "^0.11.1",
		"install": "^0.12.1",
		"json-loader": "^0.5.4",
		"jsx-loader": "^0.13.2",
		"lodash": "^4.17.3",
		"lowdb": "^1.0.0",
		"mark.js": "github:julmot/mark.js",
		"now": "^13.0.0",
		"react": "^16.5.2",
		"react-dom": "^16.5.2",
		"react-ga": "^2.1.2",
		"react-hot-loader": "^4.0.0",
		"react-router": "^4.3.1",
		"react-router-dom": "^4.2.2",
		"scrape-it": "^5.0.5",
		"serve": "^10.0.2",
		"style-loader": "^0.23.0",
		"webpack": "^4.20.2",
		"webpack-cli": "^3.1.1",
		"webpack-dev-server": "^3.1.14",
		"whatwg-fetch": "^3.0.0",
		"x-ray": "^2.0.3"
	}
}
