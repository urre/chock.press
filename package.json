{
	"name": "chockpress",
	"version": "1.0.0",
	"description": "Chocking Swedish News",
	"main": "./src/index.js",
	"author": "Urban Sanden",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git://github.com/urre/chockpress.git"
	},
	"scripts": {
		"develop": "webpack-dev-server --config ./webpack.config.dev.js --mode development",
		"build": "NODE_ENV=production webpack --config ./webpack.config.prod.js --mode production",
		"start": "serve ./build",
		"scrape": "cd src/scraper && babel-node scrape.js && babel-node match.js  && cd ../../ && cp -R ./src/data/ build/data",
		"send": "cd src/scraper/helpers/ && babel-node sendtogoogle.js",
		"clean": "cd ./build && rm -rf *.js && rm -rf index.html",
		"release": "clear && npm run clean && npm run build && npm run scrape && cp -R ./src/data/ build/data && npm run now && npm run send",
		"now": "cp now.json ./build && cd build && now && now alias && now ls --all chockpress",
		"deploy": "npm run now",
		"remove": "now rm chockpress --safe --yes"
	},
	"devDependencies": {
		"app-reset": "^1.0.2",
		"async-series": "0.0.1",
		"axios": "^0.18.0",
		"babel-core": "^6.26.0",
		"babel-loader": "^8.0.2",
		"babel-preset-env": "^1.6.1",
		"babel-preset-react": "^6.24.1",
		"chalk": "^2.4.1",
		"concat-files": "^0.1.0",
		"css-loader": "^1.0.0",
		"google-sheets-node-api": "^0.3.0",
		"google-spreadsheet": "^2.0.3",
		"google-spreadsheet-append": "0.0.7",
		"html-loader": "^0.5.5",
		"html-webpack-plugin": "^3.2.0",
		"http-server": "^0.11.1",
		"install": "^0.12.1",
		"json-loader": "^0.5.4",
		"jsx-loader": "^0.13.2",
		"lodash": "^4.17.3",
		"mark.js": "github:julmot/mark.js",
		"npm": "^6.4.1",
		"react": "^16.5.2",
		"react-dom": "^16.5.2",
		"react-ga": "^2.1.2",
		"react-hot-loader": "^4.0.0",
		"react-router": "^4.3.1",
		"react-router-dom": "^4.2.2",
		"style-loader": "^0.23.0",
		"webpack": "^4.20.2",
		"webpack-cli": "^3.1.1",
		"webpack-dev-server": "^3.1.9",
		"whatwg-fetch": "^3.0.0",
		"x-ray": "^2.0.3"
	},
	"dependencies": {
		"scrape-it": "^5.0.5",
		"serve": "^10.0.2"
	}
}
