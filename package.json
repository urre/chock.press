{
	"name": "chockpress",
	"version": "1.0.0",
	"description": "Chocking Swedish News",
	"main": "./src/index.js",
	"author": "Urban Sanden",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git://github.com/urre/chockpress.git"
	},
	"scripts": {
		"develop": "webpack-dev-server --config ./webpack.config.dev.js --mode development",
		"build": "webpack --config ./webpack.config.prod.js --mode production",
		"start": "serve ./build",
		"scrape": "cd src/scraper && babel-node scrape.js && babel-node match.js  && cd ../../ && cp -R ./src/data/ build/data",
		"send": "cd src/scraper/helpers/ && babel-node sendtogoogle.js",
		"now": "now ./build && now alias",
		"publish": "npm run build && npm run scrape && cp -R ./src/data/ build/data && npm run now && npm run send",
		"deploy": "npm run now"
	},
	"now": {
		"name": "chock.press",
		"alias": "chockpress",
		"public": true
	},
	"devDependencies": {
		"app-reset": "^1.0.2",
		"async-series": "0.0.1",
		"axios": "^0.15.3",
		"babel-core": "^6.26.0",
		"babel-loader": "^7.1.4",
		"babel-preset-env": "^1.6.1",
		"babel-preset-react": "^6.24.1",
		"chalk": "^1.1.3",
		"concat-files": "^0.1.0",
		"css-loader": "^0.28.11",
		"google-sheets-node-api": "^0.3.0",
		"google-spreadsheet": "^2.0.3",
		"google-spreadsheet-append": "0.0.7",
		"html-loader": "^0.5.5",
		"html-webpack-plugin": "^3.0.7",
		"http-server": "^0.9.0",
		"install": "^0.8.4",
		"json-loader": "^0.5.4",
		"jsx-loader": "^0.13.2",
		"lodash": "^4.17.3",
		"mark.js": "github:julmot/mark.js",
		"npm": "^4.1.1",
		"react": "^15.4.1",
		"react-dom": "^15.4.1",
		"react-ga": "^2.1.2",
		"react-hot-loader": "^4.0.0",
		"react-router": "^3.0.0",
		"react-router-dom": "^4.2.2",
		"style-loader": "^0.20.3",
		"webpack": "^4.1.1",
		"webpack-cli": "^2.0.12",
		"webpack-dev-server": "^3.1.1",
		"whatwg-fetch": "^0.11.0",
		"x-ray": "^2.0.3"
	},
	"dependencies": {
		"scrape-it": "^3.2.2",
		"serve": "^6.5.3"
	}
}
