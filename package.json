  "scripts": {
    "start": "webpack-dev-server --config webpack.config.dev.js",
    "build": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "scrape": "cd src/scraper && babel-node scrape.js && babel-node match.js  && cd ../../ && cp -R ./src/data/ build/data",
    "deploy": "npm run build && npm run scrape && cp -R ./src/data/ build/data && now ./build && bash purge.sh && npm run send",
    "send": "cd src/scraper/helpers/ && babel-node sendtogoogle.js"
  },
